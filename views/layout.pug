        if head
            title JSDaddy - JavaScript Development Company
        meta(charset='utf-8')
        meta(http-equiv='X-UA-Compatible', content='IE=edge')
        meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=2')
        if head
            each meta in head.meta
                meta(name=meta.name, content= meta.content)
        meta(property='og:image', content='path/to/image.jpg')
        meta(name='format-detection', content='telephone=no')
        meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=2, user-scalable=0')
        meta(name="Keywords" content='JS, Web Development, Full-stack, jsdaddy, JSDADDY, Angular, TypeScript, React, Redux, NodeJS')
        link(rel='shortcut icon', href='', type='image/x-icon')
        link(rel='apple-touch-icon', href='')
        link(rel='apple-touch-icon', sizes='72x72', href='')
        link(rel='apple-touch-icon', sizes='114x114', href='')
        link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/css/bootstrap.min.css', integrity='sha384-PsH8R72JQ3SOdhVi3uxftmaW6Vc51MKb0q5P2rRUpPvrszuE4W1povHYgTpBfshb', crossorigin='anonymous')
        link(type='text/css', rel='stylesheet', href='/css/slick.css')
        link(type='text/css', rel='stylesheet', href='/css/font-awesome.min.css')
        link(type='text/css', rel='stylesheet', href='/css/site.min.css')
        link(rel='icon', href='/img/favicon.ico')
        link(type='text/css', rel="stylesheet", href="/css/animate.min.css")

        script(defer, src='/js/main.min.js')
    body
        <!-- Google Tag Manager (noscript) -->
        noscript
            iframe(src="https://www.googletagmanager.com/ns.html?id=GTM-T33GBWR" height="0" width="0" style="display:none;visibility:hidden")
        <!-- End Google Tag Manager (noscript) -->

            header
                #header.default
                    .container#header_container
                        .row.align-items-center.justify-content-between#header_wrap
                            .col-2.logo
                                a(href='/')
                                    img(src=`/img/logo.png`, alt="JsDaddy")
                            .nav-wrapper.col-lg-10
                                nav
                                    ul#menu-wrap                                        
                                        li
                                            a(href='/#dev-process' class="link-nav") Development Process
                                        li
                                            a(href="/#technologies" class="link-nav") Technologies
                                        li
                                            a(href="/#testimonials" class="link-nav") Testimonials
                                        //- li
                                        //-     a(href="/#about_us" class="link-nav") #{headerMenu.nav.about}
                                        li
                                            a(href="/blog" class="link-nav") Blog
                                .btn-wrap
                                    a(href="#popup-form" class="btn contact_us_btn") Contact Us
                                h5 Visit our
                                    a(href="https://github.com/JsDaddy" target="_blank" aria-label="github" rel="noopener")
                                        i.fa.fa-github
                            .nav-burger
                                .line
                                .line-middle
                                .line
            
            block content
            footer
                .container#footer
                    .row
                        .footer-wrapper
                            .contacts
                                .contacts__logo
                                    a(href="#")
                                        img(src="/img/" + 'logo.png', alt="JsDaddy")
                                .contacts__email
                                    a(href="mailto:info@jsdaddy.io")
                                        i(class="fa fa-envelope-o", aria-hidden="true") info@jsdaddy.io
                                .contacts__skype
                                    a(href="skype:juncker8888")
                                        i(class="fa fa-skype" aria-hidden="true") juncker8888
                                .contacts__skype
                                    a(href="skype:sirnip86")
                                        i(class="fa fa-skype" aria-hidden="true") sirnip86
                                .contacts__addres Kharkiv, Ukraine
                            .navigation-footer
                                ul
                                    li
                                        a(href="/#dev-process") Development Process
                                    li
                                        a(href="/#technologies") Technologies
                                    li
                                        a(href="/#testimonials") Testimonials
                            .social
                                h5 Social
                                ul
                                    each item in [{ link: 'https://www.linkedin.com/company/jsdaddy/', className: 'linkedin', iconClass: 'fa-linkedin' }, { link: 'https://github.com/JsDaddy/', className: 'github', iconClass: 'fa-github' }]
                                        li
                                            a(href=item.link, class=item.class, target="_blank" aria-label="social" rel="noopener")
                                                i.fa(class=item['iconClass']) 

                .copy-wrapper
                    .container
                        .row
                            .col-lg-12 Copyright â’¸ 2017 - #{new Date().getFullYear()} JSDaddy

</html>
