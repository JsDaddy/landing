{
  "name": "landing",
  "version": "1.0.0",
  "description": "#### Installation ``` ## Install dart using brew to your local machine brew tap dart-lang/dart brew install dart --devel ```",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "nodemon": "nodemon",
    "start": "npm run build:css && pm2 start src/server/main.ts",
    "lint": "tslint 'src/**/*.ts'",
    "build:css": "node-sass --include-path scss src/web/sass/index.sass public/css/site.min.css --output-style compressed",
    "build:css:w": "npm run build:css -- -w",
    "build:js": "webpack",
    "build:js:w": "npm run build:js -- -w",
    "start:dev": "npm run build:css && npm run build:js && nodemon",
    "deploy": "npm run build:css && npm run build:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JsDaddy/landing.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JsDaddy/landing/issues"
  },
  "homepage": "https://github.com/JsDaddy/landing#readme",
  "dependencies": {
    "animate.css": "^3.7.2",
    "body-parser": "1.19.0",
    "bootstrap": "^4.3.1",
    "compression": "^1.7.4",
    "config": "3.2.3",
    "cors": "2.8.5",
    "express": "4.17.1",
    "font-awesome": "^4.7.0",
    "slick-carousel": "^1.8.1",
    "jquery": "^3.4.1",
    "magnific-popup": "^1.1.0",
    "mustache": "3.1.0",
    "nodemailer": "^6.3.0",
    "pug": "3.0.1",
    "wowjs": "^1.1.3"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.1",
    "@types/config": "0.0.34",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.1",
    "@types/jquery": "^3.3.31",
    "@types/mongoose": "5.5.19",
    "@types/mustache": "0.8.32",
    "@types/node": "12.7.11",
    "@types/nodemailer": "6.2.1",
    "@types/compression": "^1.0.1",
    "awesome-typescript-loader": "5.2.1",
    "compression-webpack-plugin": "^3.0.0",
    "mongoose": "5.7.3",
    "node-sass": "4.12.0",
    "nodemon": "1.19.3",
    "npm-run-all": "4.1.5",
    "pre-commit": "1.2.2",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "typescript": "3.6.3",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.9"
  },
  "pre-commit": [
    "lint"
  ]
}
